#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 9;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x2550d00 .scope module, "test" "test" 2 3;
 .timescale -9 -9;
P_0x2573ba8 .param/real "times" 2 8, Cr<m4000000000000000gfc1>; value=0.500000
v0x2585a10_0 .var "KEY", 3 0;
v0x2585ab0_0 .net "OUT_1", 0 0, v0x2585600_0; 1 drivers
v0x2585b60_0 .net "OUT_2", 0 0, v0x25856a0_0; 1 drivers
v0x2585c10_0 .net "OUT_3", 0 0, v0x2585740_0; 1 drivers
v0x2585cf0_0 .var "clk", 0 0;
v0x2585da0_0 .var "reset", 0 0;
E_0x254fde0 .event negedge, v0x2585890_0;
S_0x2570f00 .scope module, "ENCRIPT" "ENCRIPT" 2 11, 3 1, S_0x2550d00;
 .timescale 0 0;
v0x2571020_0 .net "KEY", 3 0, v0x2585a10_0; 1 drivers
v0x2585600_0 .var "OUT_1", 0 0;
v0x25856a0_0 .var "OUT_2", 0 0;
v0x2585740_0 .var "OUT_3", 0 0;
v0x25857f0_0 .var "SET", 3 0;
v0x2585890_0 .net "clk", 0 0, v0x2585cf0_0; 1 drivers
v0x2585970_0 .net "reset", 0 0, v0x2585da0_0; 1 drivers
E_0x2570ff0/0 .event negedge, v0x2585970_0;
E_0x2570ff0/1 .event posedge, v0x2585890_0;
E_0x2570ff0 .event/or E_0x2570ff0/0, E_0x2570ff0/1;
    .scope S_0x2570f00;
T_0 ;
    %wait E_0x2570ff0;
    %load/v 8, v0x2585970_0, 1;
    %inv 8, 1;
    %jmp/0xz  T_0.0, 8;
    %load/v 8, v0x25857f0_0, 1; Only need 1 of 4 bits
; Save base=8 wid=1 in lookaside.
    %ix/load 1, 1, 0;
    %mov 4, 0, 1;
    %jmp/1 T_0.2, 4;
    %load/x1p 9, v0x25857f0_0, 1;
    %jmp T_0.3;
T_0.2 ;
    %mov 9, 2, 1;
T_0.3 ;
; Save base=9 wid=1 in lookaside.
    %ix/load 1, 2, 0;
    %mov 4, 0, 1;
    %jmp/1 T_0.4, 4;
    %load/x1p 10, v0x25857f0_0, 1;
    %jmp T_0.5;
T_0.4 ;
    %mov 10, 2, 1;
T_0.5 ;
; Save base=10 wid=1 in lookaside.
    %and 9, 10, 1;
    %xor 8, 9, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v0x2585600_0, 0, 8;
    %load/v 8, v0x2585600_0, 1;
    %ix/load 1, 3, 0;
    %mov 4, 0, 1;
    %jmp/1 T_0.6, 4;
    %load/x1p 9, v0x25857f0_0, 1;
    %jmp T_0.7;
T_0.6 ;
    %mov 9, 2, 1;
T_0.7 ;
; Save base=9 wid=1 in lookaside.
    %xor 8, 9, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v0x25856a0_0, 0, 8;
    %load/v 8, v0x25856a0_0, 1;
    %load/v 9, v0x2585600_0, 1;
    %and 8, 9, 1;
    %set/v v0x2585740_0, 8, 1;
    %load/v 8, v0x2585740_0, 1;
    %load/v 9, v0x25857f0_0, 3; Select 3 out of 4 bits
    %set/v v0x25857f0_0, 8, 4;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v0x2571020_0, 4;
    %ix/load 0, 4, 0;
    %assign/v0 v0x25857f0_0, 0, 8;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x2550d00;
T_1 ;
    %delay 1, 0;
    %load/v 8, v0x2585cf0_0, 1;
    %inv 8, 1;
    %set/v v0x2585cf0_0, 8, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x2550d00;
T_2 ;
    %set/v v0x2585cf0_0, 0, 1;
    %set/v v0x2585da0_0, 1, 1;
    %movi 8, 1, 4;
    %set/v v0x2585a10_0, 8, 4;
    %movi 8, 2, 3;
T_2.0 %cmp/s 0, 8, 3;
    %jmp/0xz T_2.1, 5;
    %add 8, 1, 3;
    %wait E_0x254fde0;
    %jmp T_2.0;
T_2.1 ;
    %set/v v0x2585da0_0, 0, 1;
T_2.2 ;
    %load/v 8, v0x2585da0_0, 1;
    %jmp/0xz T_2.3, 8;
    %wait E_0x254fde0;
    %jmp T_2.2;
T_2.3 ;
    %wait E_0x254fde0;
    %movi 8, 10, 5;
T_2.4 %cmp/s 0, 8, 5;
    %jmp/0xz T_2.5, 5;
    %add 8, 1, 5;
    %wait E_0x254fde0;
    %jmp T_2.4;
T_2.5 ;
    %vpi_call 2 22 "$finish";
    %end;
    .thread T_2;
    .scope S_0x2550d00;
T_3 ;
    %vpi_call 2 26 "$dumpfile", "hoge.vcd";
    %vpi_call 2 27 "$dumpvars", 1'sb0, S_0x2550d00;
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "hoge_te.v";
    "regi.v";
